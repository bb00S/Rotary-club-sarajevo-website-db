@model List<Project>

<div class="container text-center">

    <div class="row">
        <!--potrebno dodati linkove za fb postove?-->
        @foreach (var project in Model)
        {
            <div class="col-md-4 col-xs-4">
                <div class="card75 rounded-4 shadow-4">
                    <img src="@project.PhotoUrl" class="card-img-top" alt="...">
                    <div class="card-body">
                        <h5 class="card-title mt-2">@project.Title</h5>
                        <p class="card-text">@project.Content</p>
                        <a href="" class="btn btn-primary mb-2">Više</a>
                    </div>
                </div>
            </div>

        }

    </div>
</div>